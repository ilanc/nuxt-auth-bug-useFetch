  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/users.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "name": "Attach",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"],
      "restart": true
      // "remoteRoot": "/var/task",
      // "localRoot": "${workspaceFolder}/container",
    }
  ]
}
